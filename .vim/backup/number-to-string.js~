var DualBox = require('@dualbox/dualbox');
var description = require('./package.json');

/**
 *  A simple vector substraction module for DualBox.
 *  @constructor
 *  @extends {DualBox.Module}
 */
var NumberToString = function(attrs){
    DualBox.Module.call(this, description, attrs);
    this.parallel = false;
    this.async = false;
};
NumberToString.prototype = Object.create(DualBox.Module.prototype);
NumberToString.prototype.constructor = NumberToString;

// [Abstract] See DualBox.Module
NumberToString.prototype.compute = function(input, response){

    var output = {
        s : input.x.toString()
    };

    response.send(output);
};

module.exports = NumberToString;
