const Widget3D = require('./Widget3D');


class Widget3DCamera extends Widget3D {
    constructor(pkgDesc, appDesc, options) {
        super(pkgDesc, appDesc, options);
    }

    getInitialCameraPosition() {
        throw "Error : getInitialCameraPosition must be reimplemented in all widget3D managing a canvas camera.";
    }
}

module.exports = Widget3DCamera;
